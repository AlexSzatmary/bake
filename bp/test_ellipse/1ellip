$label$;1ellip$lngx$
# I'm just going for ten timesteps.
$t_end$;1.d-2
$capillary_no$;1.d-2
$gamma_dot$;($Eh$*$capillary_no$/($radx$*$mu$))
$td$;($gamma_dot_p$/$gamma_dot$)
$umean$;(/0.d0,0.d0,0.d0/)
$bfs1$;(/0.d0, 0.d0, 0.d0/)
$bfs2$;(/0.d0, 0.d0, $gamma_dot$/2.d0/)
$bfs3$;(/0.d0, $gamma_dot$/2.d0, 0.d0/)
$nsph$;0
$ncap$;($nsph$+$nellip$)
$nellip$;1
$rad$;(/0/)
$xc_cap$;(/0.5d0*(flngx+1.d0)/)
$yc_cap$;(/0.5d0*(flngy+1.d0)/)
$zc_cap$;(/0.5d0*(flngz-1.d0)/)
$fineness$;(/5/)
$npls$;0
$pl_n1$;32
$pl_n2$;$pl_n1$
$recty$;0
$planey$;2.5d0
$lngx$;5
$FVS$;1
$gapratio$;0.00d0
$b$;4.d0
$mix$;0
$smalldumpint$;1
$bigdumpint$;5
$nu$;($mu$/$rho$)
$lcube$;($radx$*$b$)
# Density of plasma (g/(cm^3)), Neofytou 2004
$rho$;1.025d0
$dpdz$;0.d0
# This is the shear rate in the program units; it dictates the timestep.
$gamma_dot_p$;($t_end$/$nend$)
$Eh$;1.d0
$radx$;3.338d-4
# Viscosity of plasma (poise), Eggleton & Popel 1998
$mu$;1.20d-2
$nend$;10
$nstep$;10
$nm$;1.33d0
$np$;1.37d0
$omega_zero$;1.5d-6
$epsilono$;8.85418781762d-12
$lambda$;833e-9
$index$;0 
$opticalPower$;0.14d0
$lightSpeed$;299792458d0
$numberOfMaxReflections$;2
$optical$;0
$centerstable$;0
$a_prestress$;(/0.d0/)
# Specify geometry for ellipses
# ellipa is the semi-major axis length in the n1 direction
# b in the n2, and c in the n3
$ellipa$;(/$radx$/)
$ellipb$;(/$radx$/2/)
$ellipc$;(/$radx$/2/)
# Each ellipsoid has three entries in each of these arrays,
# ie, entries 1:3 in $ellipn2$ correspond to the n2 vector
# of the first ellipse, entries 4:6 correspond to the n2
# vector of the second ellipse, and so on.
$ellipn1$;(/dsqrt(2.d0)/2.d0, dsqrt(2.d0)/2.d0, 0.d0/)
$ellipn2$;(/dsqrt(2.d0)/2.d0, -dsqrt(2.d0)/2.d0, 0.d0/)
$ellipn3$;(/0.d0, 0.d0, 1.d0/)
